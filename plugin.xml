<?xml version='1.0' encoding='utf-8'?>
<plugin id="com.edifylabs.noter.testPlugin" version="1"
    xmlns="http://apache.org/cordova/ns/plugins/1.0"
    xmlns:android="http://schemas.android.com/apk/res/android">
    <name>testPlugin</name>
    <description></description>
    
    <engines>
        <engine name="cordova" version=">=2.7" />
    </engines>
    
    <js-module src="www/testPlugin.js" name="testPlugin"> </js-module>
    
    <platform name="android">
        <config-file target="platforms/android/res/xml/config.xml" parent="/plugins">
            <plugin name="testPlugin" value="testPlugin"/>
        </config-file>
        <source-file src="src/android/TestPlugin.java" target-dir="plugins/" />
    </platform>
    
    <platform name="ios">
        <config-file target="config.xml" parent="/*/plugins">
            <plugin name="TestPlugin" value="TestPlugin" onload="true" />
        </config-file>
        <config-file target="config.xml" parent="/widget">
            <feature name="TestPlugin">
                <param name="ios-package" value="TestPlugin"/>
                <param name="onload" value="true" />
            </feature>
        </config-file>
        <header-file src="src/ios/TestPlugin.h" />
        <source-file src="src/ios/TestPlugin.m" />
    </platform>
    
</plugin>